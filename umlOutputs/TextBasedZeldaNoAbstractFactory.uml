@startuml
interface Monster
{
    + announce()
    + move()
}


class NormalBokoblin
{
    NormalBokoblin()
    + announce()
    + bludgeon()
    + defend()
    + move()
}
NormalBokoblin ..|> Monster

class NormalMoblin
{
    NormalMoblin()
    + announce()
    + kick()
    + move()
    + stab()
}
NormalMoblin ..|> Monster

class NormalLizalflos
{
    NormalLizalflos()
    + announce()
    + hide()
    + move()
    + throwBoomerang()
}
NormalLizalflos ..|> Monster


@enduml